      // Function to clear local storage
      /* function clearLocalStorage() {
        localStorage.clear();
      }

      // Event listener for beforeunload
      window.addEventListener('beforeunload', clearLocalStorage);*/

      function checkAndUpdateLocalStorage() {
      // Get the current date
      const currentDate = new Date().toLocaleDateString();

      // Check if there is a stored date in local storage
      const storedDate = localStorage.getItem('storedDate');

      if (storedDate) {
        // If there is a stored date, compare it with the current date
        if (storedDate !== currentDate) {
          // Clear local storage and update with the current date
          localStorage.clear();
          localStorage.setItem('storedDate', currentDate);
          console.log('Local storage cleared and updated.');
        } else {
          console.log('Local storage is up to date.');
        }
      } else {
        // If there is no stored date, set the current date in local storage
        localStorage.setItem('storedDate', currentDate);
        console.log('Initial local storage set.');
      }
    }
    // Call the function on page load
    window.onload = checkAndUpdateLocalStorage;

      function updateUrl(element) {
        const dataSrc = element.getAttribute('data-src');
        const dataDownload = element.getAttribute('data-download');
        const finalPath = dataSrc.replace(".pdf", "");

        //console.log(dataDownload)
        
        if (localStorage.getItem('pdfUrl') !== null) {
          const gettotalPdf = JSON.parse(localStorage.getItem('pdfUrl'));
          const gettotalPdfCount = Number(gettotalPdf?.length)+1;
          
          gettotalPdf.push({'path':finalPath,'index':gettotalPdfCount,'dataDownload':dataDownload});
          localStorage.setItem('pdfUrl', JSON.stringify(gettotalPdf));

          window.open('https://jyoti.co.in/pdf-viewer?'+gettotalPdfCount, '_blank');
        }else{
          localStorage.setItem('pdfUrl', JSON.stringify([{path:finalPath,index:1,'dataDownload':dataDownload}]));

          window.open('https://jyoti.co.in/pdf-viewer?1', '_blank');
        }
      }

      /* function updateUrl(element) {
        const dataSrc = element.getAttribute('data-src');
        const finalPath = dataSrc.replace(".pdf", "");
        localStorage.setItem('pdfUrl', finalPath);
        window.open('https://jyoti.co.in/pdf-viewer/index.php', '_blank');
      } */

      $(document).ready(function() {
         // Get the user's IP address on page load for each set
         $('.ip').each(function(index, element) {
            var setId = $(element).attr('id').replace('GettoIP', '');
            $.getJSON("https://api.ipify.org?format=json", function(data) {
               $("#GettoIP" + setId).val(data.ip);
            });
         });

         $('.send').on('click', function() {
            var setId = $(this).data('set');
            var ip = $('#GettoIP' + setId).val();
            let ButtonName = $(this).text();
            var windowHeight = $(window).height();
            var windowWidth = $(window).width();
            var currentAddress = window.location.href;
            //event.preventDefault();
            //var linkPath = $(this).attr('href');
            var linkPath = $(this).attr('data-src');

            // Fetch geolocation information based on the provided IP address
            $.getJSON('https://ipapi.co/' + ip + '/json', function(data) {
               $('.city' + setId).text(data.city);
               $('.region' + setId).text(data.region);
               $('.postal' + setId).text(data.postal);
               $('.country' + setId).text(data.country_name);
               $('.latitude' + setId).text(data.latitude);
               $('.longitude' + setId).text(data.longitude);

               // Fetch device name
               var deviceName = getDeviceName();
               var deviceNameNew = getDeviceNameNew();
               var checkDeviceType = getcheckDeviceType();
               // Display device name in the respective span
               $('#DeviceName' + setId).text(deviceName);
               $('#DeviceNameNew' + setId).text(deviceNameNew);
               $('#checkDeviceType' + setId).text(checkDeviceType);
               $('#currentAddress' + setId).text(currentAddress);
               $('#linkPath' + setId).text(linkPath);
               $('#DeviceWidthHeight' + setId).text('Width ' + windowWidth + ' x Height ' + windowHeight + ' pixels');
               var DeviceWidthHeight = 'Width ' + windowWidth + ' x Height ' + windowHeight + ' pixels';
               // Send data to the server for insertion
               $.ajax({
                     url: 'https://jyoti.co.in/inv-doc-report/insert_data.php',
                     method: 'POST',
                     data: {
                        ip: ip,
                        city: data.city,
                        region: data.region,
                        postal: data.postal,
                        country: data.country_name,
                        latitude: data.latitude,
                        longitude: data.longitude,
                        ButtonName: ButtonName,
                        deviceName: deviceName[0],
                        //deviceName: deviceName.length > 0 ? deviceName[0].deviceName : [1],
                        deviceNameNew: deviceNameNew,
                        checkDeviceType: checkDeviceType,
                        currentAddress: currentAddress,
                        linkPath: linkPath,
                        DeviceWidthHeight: DeviceWidthHeight,
                     },
                     success: function(response) {
                        console.log(response);
                     }
               });
            });
         });

         function getDeviceName() {
            return navigator.userAgent.match(/^[^\(]+\((\w+)/); // You can use other properties of navigator to get more specific information
            //return navigator.userAgent; // You can use other properties of navigator to get more specific information
         }

         function getDeviceNameNew() {
            const UserAgent = navigator.userAgent
            const device = {
               iPhone: /iPhone/.test(UserAgent),
               iPad: /iPad/.test(UserAgent),
               iPod: /iPod/.test(UserAgent),
               Android: /Android/.test(UserAgent),
               BlackBerry: /BlackBerry/.test(UserAgent),
               WindowsPhone: /Windows Phone|IEMobile/.test(UserAgent),
               Kindle: /Kindle|Silk/.test(UserAgent),
               SmartTV: /SmartTV/.test(UserAgent),
               Macintosh: /Macintosh/.test(UserAgent),
               Windows: /Windows/.test(UserAgent),
               Linux: /Linux/.test(UserAgent),
               ChromeOS: /CrOS/.test(UserAgent),
               Nexus: /Nexus/.test(UserAgent),
               Samsung: /Samsung/.test(UserAgent),
               LG: /LG/.test(UserAgent),
               Sony: /Sony/.test(UserAgent),
               HTC: /HTC/.test(UserAgent),
               Nokia: /Nokia/.test(UserAgent),
               Motorola: /Motorola/.test(UserAgent),
               OnePlus: /OnePlus/.test(UserAgent),
               Huawei: /Huawei/.test(UserAgent),
               Xiaomi: /Xiaomi/.test(UserAgent),
               Oppo: /Oppo/.test(UserAgent),
               Vivo: /Vivo/.test(UserAgent),
               GooglePixel: /Pixel/.test(UserAgent),
               Surface: /Surface/.test(UserAgent),
               PlayStation: /PlayStation/.test(UserAgent),
               Xbox: /Xbox/.test(UserAgent),
               Nintendo: /Nintendo/.test(UserAgent)
            }
         
            for(VDeviceName in device){
               //console.log(device[VDeviceName]);
               if(device[VDeviceName])
               //document.writeln("You are using " + VDeviceName);
               //var device = "You are using " + VDeviceName();
               //$('#deviceInfo').text(VDeviceName);
               return VDeviceName;
         
            }
         }

         function getcheckDeviceType() {
            // Check if the device is a tablet
            if (navigator.userAgent.match(/iPad|Android|Tablet/i)) {
              return 'Tablet';
            }
            // Check if the device is a smartphone
            else if (navigator.userAgent.match(/iPhone|Android/i)) {
              return 'Smartphone';
            }
            // Assume it's a computer if not a tablet or smartphone
            else {
              return 'Computer';
            }
         }
   });</script>  <script>jQuery(document).on("click", ".mainNavProduct", function() {
